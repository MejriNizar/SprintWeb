{% extends 'FOSMessageBundle::layout.html.twig' %}

{% block fos_message_content %}

<h1>{% trans from 'FOSMessageBundle' %}send new message{% endtrans %}</h1>

<form action="{{ url('fos_message_thread_new') }}" method="post">
    {{ form_widget(form) }}


    <div class="row form-group">
        <div class="col-md-12">
            <input type="submit" value="Send" class="btn btn-primary py-2 px-4 text-white">
        </div>

    </div>
</form>

{% endblock %}
