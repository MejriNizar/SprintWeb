
{% extends 'base.html.twig' %}


{% block body %}





    <div class="site-section bg-light">
        <div class="container">
            <div class="row align-items-stretch">






                {% for d in demandes | reverse %}
                <div class="col-md-6 col-lg-4 mb-4 mb-lg-4">
                    <div class="unit-4 d-flex">
                        <div class="unit-4-icon mr-4"><span class="text-primary icon-message"></span></div>
                        <div>
                            <h3>{{ d.budget }}</h3>
                            <p>{{ d.description }}</p>
                            <p><a href="#">Learn More</a></p>
                        </div>
                    </div>
                </div>
            {% endfor %}
            </div>
        </div>
    </div>









{% endblock %}

